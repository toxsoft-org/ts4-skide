<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="org.toxsoft.skide.exe.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi"
            apply="always">
      </fragment>
   </extension>

   <extension
         id="skide_product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            description="Skat IDE launcher"
            name="SkIDE">
         <property
               name="applicationXMI"
               value="platform:/plugin/org.toxsoft.tsgui.mws/Application.e4xmi">
         </property>
         <property
               name="appName"
               value="SkIDE">
         </property>
      </product>
   </extension>

</plugin>
